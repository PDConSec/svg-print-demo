{
	"name": "svg-print-demo",
	"displayName": "SVG Print Demo",
	"description": "A demonstration of how to write a preview extension that can print.",
	"version": "0.0.5",
	"author": {
		"name": "Peter Wone",
		"email": "peter.wone@outlook.com"
	},
	"publisher": "pdconsec",
	"repository": {
		"type": "git",
		"url": "https://github.com/PDConSec/svg-print-demo"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:svg"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "svg-print-demo.preview",
				"title": "SVG preview"
			}
		],
		"configuration": [
			{
				"title": "Printing",
				"properties": {
					"print.renderSvg": {
						"type": "boolean",
						"default": true,
						"description": "Print the image instead of the code."
					}
				}
			}
		],
		"languages": [
			{
				"id": "svg",
				"extensions": [
					".svg"
				],
				"aliases": [
					"Scalable Vector Graphics"
				]
			}
		]
	},
	"extensionDependencies": [
		"pdconsec.vscode-print"
	],
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"compile": "npm run webpack",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"css-loader": "^6.3.0",
		"eslint": "^7.9.0",
		"file-loader": "^6.2.0",
		"glob": "^7.1.6",
		"html-loader": "^3.1.2",
		"mocha": "^8.1.3",
		"ts-loader": "^9.4.1",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"winston": "^3.8.2"
	},
	"dependencies": {
		"gulp": "^4.0.2"
	}
}
